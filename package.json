{
  "name": "react-conditional-wrap",
  "version": "0.0.1",
  "description": "Conditionally wrap react components",
  "main": "dist/bundle.min.js",
  "scripts": {
    "test": "jest",
    "build": "npm run build:minified && npm run build:default",
    "build:default": "MODE=default rollup --config rollup.config.js",
    "build:minified": "MODE=minified rollup --config rollup.config.js",
    "flow": "flow",
    "lint": "eslint ./src/index.js",
    "clean": "rimraf dist/",
    "prepublish": "npm run clean && npm run build"
  },
  "keywords": [
    "conditional",
    "react",
    "component",
    "wrap",
    "hoc"
  ],
  "author": "Nick Schmidt <n@nick.qa>",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "^0.69.0",
    "jest": "^22.4.3",
    "jsdom": "^11.7.0",
    "prettier": "^1.12.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-uglify": "^3.0.0",
    "sinon": "^4.5.0"
  },
  "peerDependencies": {
    "react": "^16.3.1"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./test/setup.js",
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  }
}
